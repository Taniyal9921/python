myFruits = ["apple", "banana", "cherry"]
# print(myFruits)
myFruits.insert(2,'Kiwi')
print(myFruits)
myFruits.append('orange')
print(myFruits)
myFruits.remove('Kiwi')
print(myFruits)
myFruits.pop(1)
print(myFruits)
myFruits.insert(1, 'Banana')
myFruits.pop()
print(myFruits)
myFruits.insert(3,'Orange')
print("The Array is: ", myFruits)
print("The length of the array is: ", len(myFruits))